define gef-load
    python
import gdb
import urllib.request as u, tempfile as t
g = t.NamedTemporaryFile(suffix='-gef.py')
open(g.name, 'wb+').write(u.urlopen('https://tinyurl.com/gef-main').read())
gdb.execute('source %s' %g.name)
    end
end

